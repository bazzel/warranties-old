= content_tag_for(:li, warranty) do
  = link_to(image_tag(warranty.warranty_url(:thumb)), warranty)
  .caption
    %h5= warranty.name
    %dl
      %dt Expires on:
      %dd= warranty.expires_on
    %p
      = link_to "Edit", edit_warranty_path(warranty), :class => 'btn'
      = link_to "Destroy", warranty, :method => :delete, :confirm => "Are you sure?", :class => 'btn btn-danger'
