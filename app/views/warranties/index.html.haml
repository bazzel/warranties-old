%section
  %aside
    %ul.nav.nav-list
      %li
        = link_to "Add Warranty", new_warranty_path

  %section
    %ul.warranties
      - @warranties.each do |warranty|
        = content_tag_for(:li, warranty) do
          = link_to(image_tag(warranty.warranty_url(:thumb)), warranty)
          %h5= warranty.name
          .expires_on
            %span Expires on:
            = warranty.expires_on
